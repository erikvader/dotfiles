clear lock
clear control
remove mod4 = Hyper_L
clear shift

keycode 66 = Control_L
keycode 9 = Escape Caps_Lock
keycode 62 = Hyper_L
keycode 23 = Shift_R
keycode 105 = Shift_L

keycode any = Tab
! keycode any = Escape
! keycode any = Shift_R

add control = Control_L
add mod3 = Hyper_L
add shift = Shift_R Shift_L

! switch places of buttons on acer swift 3 laptop
keycode 112 = Home
keycode 117 = End
keycode 110 = Prior
keycode 115 = Next

